{
  "name": "@pedaki/common",
  "version": "0.0.0",
  "author": "Nathan David <me@vahor.fr>",
  "repository": {
    "type": "git",
    "url": "https://github.com/PedakiHQ/pedaki",
    "directory": "packages/common"
  },
  "homepage": "https://www.pedaki.fr",
  "license": "MIT",
  "private": false,
  "type": "module",
  "exports": {
    "./cache": {
      "types": "./dist/cache/index.d.ts",
      "import": "./dist/cache/index.js",
      "require": "./dist/cache/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write . --cache",
    "format:check": "prettier --check . --cache"
  },
  "devDependencies": {
    "@pedaki/eslint-config": "0.0.4",
    "@pedaki/prettier-config": "0.0.5",
    "@types/memory-cache": "latest",
    "@types/react": "^18.2.21",
    "react": "^18.2.0",
    "tsup": "^7.2.0"
  },
  "peerDependencies": {
    "@node-rs/bcrypt": "^1.7.3",
    "memory-cache": "^0.2.0",
    "sonner": "^0.7.0",
    "nanoid": "^4.0.2"
  },
  "prettier": "@pedaki/prettier-config"
}
